<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.demo.dao.UsersMapping">

    <!--sql 语句   id对应方法名  resultType 对应上实体层 -->
    <select id="Login" resultType="com.example.demo.model.Users">

        select *from users where g_id = #{g_id} and g_pwd = #{g_pwd}
    </select>

    <insert id="Register" >
        insert into users (g_id, g_pwd) value (#{g_id},#{g_id})
    </insert>
</mapper>